; visible strings for odd characters  V0.00    1988  Tony Tebby  QJUMP

        section cv

        xdef    cv_vodds
;+++
; Table of visible strings for odd keys.
; 4 (positive) relative byte pointer to byte length strings for $fc to $ff
;---
cv_vodds
        dc.b    fc-*
        dc.b    fd-*
        dc.b    fe-*
        dc.b    ff-*
fc      dc.b    6,'sSPACE'
fd      dc.b    4,'sTAB'
fe      dc.b    6,'sENTER'
ff      dc.b    3,'ALT'
        end
