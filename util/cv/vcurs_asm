; visible strings for cursor/caps   V0.00    1988  Tony Tebby  QJUMP

        section cv

        xdef    cv_vcurs

;+++
; Table of visible strings for cursor keys and caps.
; 5 (positive) relative byte pointer to byte length strings.
;---
cv_vcurs
        dc.b    left-*
        dc.b    right-*
        dc.b    up-*
        dc.b    down-*
        dc.b    caps-*
left    dc.b    1,$bc
right   dc.b    1,$bd
up      dc.b    1,$be
down    dc.b    1,$bf
caps    dc.b    4,'CAPS'
        end
