; visible strings for functions  V0.00    1988  Tony Tebby  QJUMP

        section cv

        xdef    cv_vfunc
;+++
; Table of visible strings for function keys.
; 5 (positive) relative byte pointer to byte length strings for f1 to f5
;---
cv_vfunc
        dc.b    f1-*
        dc.b    f2-*
        dc.b    f3-*
        dc.b    f4-*
        dc.b    f5-*
f1      dc.b    2,'F1'
f2      dc.b    2,'F2'
f3      dc.b    2,'F3'
f4      dc.b    2,'F4'
f5      dc.b    2,'F5'
        end
