        section cv

        xdef    cv_vcacs
;+++
; Table of visible strings for plain, ALT, CTRL, ALT CTRL, SHIFT, ALT SHIFT etc
; 8 (positive) relative byte pointer to byte length strings
;---
cv_vcacs
null    dc.b    null-*
        dc.b    a-*
        dc.b    c-*
        dc.b    ac-*
        dc.b    s-*
        dc.b    as-*
        dc.b    cs-*
        dc.b    acs-*
a       dc.b    1,'a'
c       dc.b    1,'c'
ac      dc.b    2,'ac'
s       dc.b    1,'s'
as      dc.b    2,'as'
cs      dc.b    2,'cs'
acs     dc.b    3,'acs'
        end

