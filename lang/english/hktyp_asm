; Make a list of Executable hotkeys     V0.00   1989  Tony Tebby   QJUMP

        section language

        xdef    cv_hktyp

        dc.l    'KEY '
        dc.l    'KEY '
        dc.l    'KEY '
        dc.l    'KEY '
;+++
; A list of long words representing hotkey types. CV_HKTYP is the address
; of type 0.
;---
cv_hktyp
        dc.l    'CMD '
        dc.l    'EXEC'
        dc.l    'EXEC'
        dc.l    'LOAD'
        dc.l    'PICK'
        dc.l    'WAKE'
        dc.l    'WAKE'
        end
