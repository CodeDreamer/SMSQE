; Format date characters           1988   Tony Tebby   QJUMP

        section language

        xdef    cv_fditb
        xdef    cv_fdstb

        include 'dev8_keys_dat'

cv_fditb dc.b   'C',dat_cent
         dc.b   'A',dat_year
         dc.b   'J',dat_iday
         dc.b   'H',dat_hour
         dc.b   'M',dat_mint
         dc.b   'S',dat_sec
         dc.w   -1
cv_fdstb dc.b   'J',dat_sday
         dc.b   'M',dat_mnth
         dc.w   -1
         end
