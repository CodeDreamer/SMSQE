1000 DEFine PROCedure sa
1010   SAVE_O dev8_smsq_gold_make_bas
1020 END DEFine sa
1030 :
1040 DEFine PROCedure compile_and_check (a$)
1050 LOCal chan%,is_ok$,ch_len
1060   EXEP_W "make";a$&" -w"
1070   is_ok$=a$(1 TO LEN(a$)-4)
1080   chan%=FOP_IN(is_ok$&'log'):  REMark open log file
1090   IF chan%<0
1100      PRINT a$& " : "&"couldn't open log file"
1110      RETurn
1120   END IF
1130   ch_len=FLEN(#chan%)
1140   GET#chan%\ch_len-10
1150   INPUT#chan%,is_ok$
1160   CLOSE#chan%
1170   IF NOT is_ok$=="No errors"
1180     PRINT a$ & " : Compile/link error!"
1190   END IF
1200 END DEFine compile_and_check
1210 :
