560 DEFine PROCedure make_it
570   compile_and_check 'dev8_smsq_gold_host_ \b \0 all \1 \2'
580   compile_and_check 'dev8_smsq_gold_hwinit_ \b \0 all \1 \2'
590   compile_and_check 'dev8_smsq_gold_nasty_ \b \0 all \1 \2'
600   compile_and_check 'dev8_smsq_gold_nasty_s_ \b \0 all \1 \2'
610   compile_and_check 'dev8_smsq_gold_driver_ql_ \b \0 all \1 \2'
620   compile_and_check 'dev8_smsq_gold_driver_most_ \b \0 all \1 \2'
630   compile_and_check 'dev8_smsq_gold_driver_nd_ \b \0 all \1 \2'
640   compile_and_check 'dev8_smsq_gold_driver_nds_ \b \0 all \1 \2'
650   compile_and_check 'dev8_smsq_gold_driver_dv3_ \b \0 all \1 \2'
660   compile_and_check 'dev8_smsq_gold_kbd_lang_ \b \0 all \1 \2'
670   compile_and_check 'dev8_smsq_gold_kbd_abc_lng_ \b \0 all \1 \2'
