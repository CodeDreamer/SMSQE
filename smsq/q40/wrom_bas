100 INPUT 'Filename ';fn$
110 f1$ = 'win1_smsq_q40_'&fn$
120 flo$ = 'flp1_'&fn$&'LO'
130 fhi$ = 'flp1_'&fn$&'HI'
140 OPEN_IN #5,f1$
150 OPEN_OVER #6,'win1_lo'
160 OPEN_OVER #7,'win1_hi'
200 REPeat
210  IF EOF(#5): EXIT
220  BGET #5,a,b: BPUT #7,b,a
230  IF EOF(#5): EXIT
240  BGET #5,a,b: BPUT #6,b,a
250 END REPeat
260 CLOSE #5,#6,#7
270 COPY_O 'win1_lo' TO flo$
280 COPY_O 'win1_hi' TO fhi$
