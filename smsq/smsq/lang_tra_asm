tra_deu
	dc.w	49
	dc.w	tra_deu1-tra_deu
	dc.w	tra_deu2-tra_deu

tra_deu1
	dc.b	$00,$01,$02,$03,$04,$05,$06,$07
	dc.b	$08,$09,$0A,$0B,$0C,$0D,$0E,$0F
