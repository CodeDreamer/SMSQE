        xref    hkc_english

        section defs

        xdef    ynaq
        xdef    message

ynaq     dc.b    'YNAQ'
message
         dc.w   yn_messg-*
         dc.w   ynaq_mes-*
         dc.w   to_messg-*
         dc.w   exist_ms-*
         dc.w   overw_ms-*

yn_messg dc.w   10,'..Y or N? '
ynaq_mes dc.w   11,'..Y/N/A/Q? '
to_messg dc.w   04,' TO '
exist_ms dc.w   09,' exists, '
overw_ms dc.w   15,'OK to overwrite'
        end
