        xref    hkc_german

        section defs

        xdef    ynaq
        xdef    message

ynaq     dc.b    'JNAQ'
message
         dc.w   yn_messg-*
         dc.w   ynaq_mes-*
         dc.w   to_messg-*
         dc.w   exist_ms-*
         dc.w   overw_ms-*

yn_messg dc.w   12,'..J oder N? '
ynaq_mes dc.w   11,'..J/N/A/Q? '
to_messg dc.w   04,' TO '
exist_ms dc.w   16,' gibt es schon, '
overw_ms dc.w   20,'OK zum ‡berschreiben'
        end
