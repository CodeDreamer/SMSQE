	section utility
	xdef	mes_msleft
	xref	mes_zero
mes_msleft
	dc.w	$0100,$0000
	dc.w	7,8,0,0
	dc.l	mcs_msright-*
	dc.l	mes_zero-*
	dc.l	0
mcs_msright
	dc.w	$7C00,$0000
	dc.w	$F260,$0000
	dc.w	$F260,$0000
	dc.w	$FE00,$0000
	dc.w	$8200,$0000
	dc.w	$8200,$0000
	dc.w	$8200,$0000
	dc.w	$7C00,$0000

*
	end
