	section utility
	xdef	mes_msright
	xref	mes_zero
mes_msright
	dc.w	$0100,$0000
	dc.w	7,8,0,0
	dc.l	mcs_msleft-*
	dc.l	mes_zero-*
	dc.l	0
mcs_msleft
	dc.w	$7C00,$0000
	dc.w	$9E0C,$0000
	dc.w	$9E0C,$0000
	dc.w	$FE00,$0000
	dc.w	$8200,$0000
	dc.w	$8200,$0000
	dc.w	$8200,$0000
	dc.w	$7C00,$0000

*
	end
