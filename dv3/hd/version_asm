; Floppy Disk Version

        section version

        xdef    hd_vers
        xdef    hd.rev

; Version 3.00   Derived from Atari FD V2.18
;         3.01   Combined QXL Atari Version

hd_vers equ     '3.00'
hd.rev  equ     0

hd_vmess
        dc.w    'QXL Floppy Disk  V'
        dc.l    hd_vers
        dc.b    ' ',$a
hd_vmend
        ds.w    0
        end
