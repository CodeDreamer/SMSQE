; DV3 Floppy Disk Version

	section version

	xdef	fd_vers
	xdef	fd.rev

; Version 3.00	 Derived from Atari FD V2.18
;	  3.01	 Combined QXL Atari Version
;	  3.02	 Rationalised version with common routines

fd_vers equ	'3.02'
fd.rev	equ	0

fd_vmess
	dc.w	'QXL Floppy Disk  V'
	dc.l	fd_vers
	dc.b	' ',$a
fd_vmend
	ds.w	0
	end
