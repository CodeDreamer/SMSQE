; Empty sprite V 1.00
	section sprite
;
	xdef	sp_zero
;
sp_zero
	dc.w	$0100,$0000	4 colour mode
	dc.w	1,1,0,0 	1*1 pixel sprite origin at 0,0
	dc.l	sp_z-*
	dc.l	sp_z-*
	dc.l	s8_zero-*
s8_zero
	dc.w	$0101,$0000	8 colour mode
	dc.w	1,1,0,0 	1*1 pixel sprite origin at 0,0
	dc.l	sp_z-*
	dc.l	sp_z-*
sp_z
	dc.l	0

	end
