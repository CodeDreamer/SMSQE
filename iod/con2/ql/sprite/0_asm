	dc.w	%0111111101111111,%1111000011110000
	dc.w	%1111111111111111,%1111100011111000
	dc.w	%1111111111111111,%1111100011111000
	dc.w	%1111111111111111,%1111100011111000
	dc.w	%1111111111111111,%1111100011111000
	dc.w	%1111111111111111,%1111100011111000
	dc.w	%1111111111111111,%1111100011111000
	dc.w	%0111111101111111,%1111000011110000
