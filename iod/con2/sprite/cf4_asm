* Sprite cf4
*
*	Mode 4
*	+|----------+
*	- gg ggg  g -
*	|g   g	 gg |
*	|g   g	g g |
*	|g   gg g g |
*	|g   g ggggg|
*	|g   g	  g |
*	| gg g	  g |
*	+|----------+
*
	section sprite
	xdef	sp_cf4
	xref	sp_zero
sp_cf4
	dc.w	$0100,$0000
	dc.w	11,7,0,0
	dc.l	sc4_cf4-*
	dc.l	sp_zero-*
	dc.l	0
sc4_cf4
	dc.w	$6e00,$4000
	dc.w	$8800,$c000
	dc.w	$8900,$4000
	dc.w	$8d00,$4000
	dc.w	$8b00,$e000
	dc.w	$8800,$4000
	dc.w	$6800,$4000
