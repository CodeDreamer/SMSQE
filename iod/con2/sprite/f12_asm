* Sprite f12
*
*	Mode 4
*	+|-----------+
*	-ggg  g  ggg -
*	|g   gg g  g |
*	|g    g    g |
*	|gg   g   g  |
*	|g    g  g   |
*	|g    g g    |
*	|g    g gggg |
*	+|-----------+
*
	section sprite
	xdef	sp_f12
	xref	sp_zero
sp_f12
	dc.w	$0100,$0000
	dc.w	10,7,0,0
	dc.l	sc4_f12-*
	dc.l	sp_zero-*
	dc.l	0
sc4_f12
	dc.w	$E900,$8000
	dc.w	$9A00,$4000
	dc.w	$8800,$4000
	dc.w	$C800,$8000
	dc.w	$8900,$0000
	dc.w	$8A00,$0000
	dc.w	$8B00,$C000
*
	end
