* Sprite f8
*
*	Mode 4
*	+|-------+
*	-ggg  gg -
*	|g   g	g|
*	|g   g	g|
*	|gg   gg |
*	|g   g	g|
*	|g   g	g|
*	|g    gg |
*	+|-------+
*
	section sprite
	xdef	sp_f8
	xref	sp_zero
sp_f8
	dc.w	$0100,$0000
	dc.w	8,7,0,0
	dc.l	sc4_f8-*
	dc.l	sp_zero-*
	dc.l	0
sc4_f8
	dc.w	$E600,$0000
	dc.w	$8900,$0000
	dc.w	$8900,$0000
	dc.w	$C600,$0000
	dc.w	$8900,$0000
	dc.w	$8900,$0000
	dc.w	$8600,$0000
*
	end
