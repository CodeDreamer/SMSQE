* Sprite f4
*
*	Mode 4
*	+|-------+
*	-ggg g	 -
*	|g   g g |
*	|g   g g |
*	|gg  gggg|
*	|g     g |
*	|g     g |
*	|g     g |
*	+|-------+
*
	section sprite
	xdef	sp_f4
	xref	sp_zero
sp_f4
	dc.w	$0100,$0000
	dc.w	8,7,0,0
	dc.l	sc4_f4-*
	dc.l	sp_zero-*
	dc.l	0
sc4_f4
	dc.w	$E800,$0000
	dc.w	$8A00,$0000
	dc.w	$8A00,$0000
	dc.w	$CF00,$0000
	dc.w	$8200,$0000
	dc.w	$8200,$0000
	dc.w	$8200,$0000
*
	end
