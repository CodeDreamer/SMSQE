* Sprite f2
*
*	Mode 4
*	+|------+
*	-ggg gg -
*	|g  g  g|
*	|g     g|
*	|gg   g |
*	|g   g	|
*	|g  g	|
*	|g  gggg|
*	+|------+
*
	section sprite
	xdef	sp_f2
	xref	sp_zero
sp_f2
	dc.w	$0100,$0000
	dc.w	7,7,0,0
	dc.l	sc4_f2-*
	dc.l	sp_zero-*
	dc.l	0
sc4_f2
	dc.w	$EC00,$0000
	dc.w	$9200,$0000
	dc.w	$8200,$0000
	dc.w	$C400,$0000
	dc.w	$8800,$0000
	dc.w	$9000,$0000
	dc.w	$9E00,$0000
*
	end
